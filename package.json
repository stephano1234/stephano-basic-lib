{
  "name": "stephano-basic-lib-doc",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "watch-lib": "ng build stephano-basic-lib --watch",
    "prebuild-lib": "node ./src/scripts/generate-styles-from-tokens.js",
